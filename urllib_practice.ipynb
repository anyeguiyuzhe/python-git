   "source": [
    "#简单的url请求，打开百度\n",
    "from urllib import request\n",
    "with request.urlopen('https://www.baidu.com') as baidu:\n",
    "    data=baidu.read()\n",
    "    header=baidu.getheaders()\n",
    "    print(header)\n",
    "    print(data.decode())\n",
    "    for x,y in header:\n",
    "        print((x,y))"
   ]
